<mvc:View
    controllerName="qrcodedownload.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
					 <VBox>
	                  <Image class="qrCodfe" id="qrImageId"/>
	                  <Button press="onButtonPress" text="Pdf Download" type="Emphasized" class="pdfButton"/>
					  <Button id="FC7" text="New Property" press="showhide"/>
	              </VBox>
				  <GenericTile id="genericTile"
				
				frameType="TwoByOne" press="press">
				<TileContent footer="August 21, 2016">
					<NewsContent
						contentText="Wind Map: Monitoring Real-Time and Fore-casted Wind Conditions across the Globe"
						subheader="Today, SAP News" />
				</TileContent>
			</GenericTile>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
